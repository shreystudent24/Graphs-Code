#include <iostream>
#include<queue>
using namespace std;
void print(int** edges, int n, int sv,int *visited)
{	
    if(n == 0)
    {
        return;
    }
    queue<int> pendingVertices;
//     bool* visited = new bool[n];

//     for(int i=0; i<n; i++){
//         visited[i]=false;
//     }

    pendingVertices.push(sv);
    visited[sv]=1;

    while(!pendingVertices.empty())
    {
        int currentVertice = pendingVertices.front();
        pendingVertices.pop();
        cout<< currentVertice << " ";

        for(int i=0 ; i<n; i++)
        {

            if(currentVertice==i)
            {
                continue;
            }

            if(edges[currentVertice][i]==1 && !visited[i])
            {
                pendingVertices.push(i);
                visited[i]=1;
            }

        }
    }

}
void bfs_disconnected(int **edges,int n){
	int *visited = new int[n];
    for(int i = 0;i < n;i ++)
    {
        visited[i] = 0;
    }
    for(int i = 0;i < n;i ++)
    {
        if(visited[i] == 0)
        {
            print(edges,n,i,visited);
        }
    }
}
int main() {
    // Write your code here
    int n;
    int e;
    cin >> n >> e;

    int** edges = new int*[n];
    for(int i=0; i<n; i++){
        edges[i]=new int[n];
        for(int j=0; j<n; j++){
            edges[i][j]=0;
        }
    }

    for(int i=0; i<e; i++){
        int f,s;
        cin >> f >> s;
        edges[f][s]=1;
        edges[s][f]=1;
    }
    bfs_disconnected(edges,n);
    // if(n!=0)
    //     print(edges, n, 0);
    // Delete all the memory

}
